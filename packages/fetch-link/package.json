{
  "name": "@windyroad/fetch-link",
  "version": "2.0.0",
  "description": "A library that adapts the fetch API to work with RFC8288 Link objects.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run compile",
    "clean": "rimraf -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "prepublishOnly": "npm run build",
    "test": "vitest --run",
    "ci:test": "vitest run --config vite.config.ci.ts --coverage",
    "lint": "xo --fix --prettier"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/windyroad/very-fetching.git"
  },
  "keywords": [
    "fetch",
    "link",
    "rfc8288",
    "web linking",
    "hypermedia"
  ],
  "author": {
    "name": "Windy Road Technology",
    "url": "https://windyroad.com.au"
  },
  "contributors": [
    "Tom Howard <tom@windyroad.com.au>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/windyroad/very-fetching/issues"
  },
  "homepage": "https://github.com/windyroad/very-fetching/packages/fetch-link#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@windyroad/wrap-fetch": "*",
    "@windyroad/adapt-fetch-inputs": "*",
    "@windyroad/decorate-fetch-response": "*",
    "http-link-header": "^1.1.1"
  },
  "devDependencies": {
    "@types/http-link-header": "^1.0.3",
    "fast-check": "^3.10.0",
    "msw": "^1.2.2",
    "vitest": "^0.32.4"
  }
}
