{
  "name": "@windyroad/very-fetching",
  "version": "1.0.0",
  "description": "Very fetching utility libraries for fetch",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "xo --fix",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "process:build": "turbo run build",
    "ci:lint": "xo",
    "ci:test": "vitest --config vite.config.ci.ts --run --coverage",
    "ci:publish": "[ \"$CI\" = true ] && changeset version && changeset publish || echo \"Dry run: changeset version && changeset publish\"",
    "ci:process:build": "turbo run ci:build",
    "ci:process:publish": "turbo run ci:publish",
    "release": "changeset publish "
  },
  "engines": {
    "node": "18"
  },
  "keywords": [
    "fetch"
  ],
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "author": {
    "name": "Windy Road Technology",
    "url": "https://windyroad.com.au"
  },
  "contributors": [
    "Tom Howard <tom@windyroad.com.au>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@vitest/coverage-v8": "^0.32.2",
    "@windyroad/xo-config": "*",
    "adr": "^1.4.5",
    "eslint": "^8.44.0",
    "eslint-plugin-jsdoc": "^46.4.3",
    "globby": "^13.2.2",
    "turbo": "1.10.5",
    "typescript": "^5.1.6",
    "vitest": "^0.32.4",
    "xo": "^0.54.2"
  }
}
