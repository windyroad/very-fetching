    "@windyroad/xo-config": "*",
    "eslint": "^8.44.0",
    "eslint-plugin-jsdoc": "^46.4.3",
    "globby": "^13.2.2",
    "semantic-release": "^20.1.3",
    "semantic-release-monorepo": "^7.0.5",
    "turbo": "1.10.5",
    "typescript": "^5.1.6",
