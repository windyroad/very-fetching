{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "inputs": [
                "src/**/*"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "lint": {}
    },
    "globalEnv": [
        "FORCE_COLOR"
    ]
}